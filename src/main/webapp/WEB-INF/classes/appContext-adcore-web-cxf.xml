<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:jaxws="http://cxf.apache.org/jaxws"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd  
	http://cxf.apache.org/jaxws 
	http://cxf.apache.org/schema/jaxws.xsd">
	
	<bean id="adWsService" class="com.sogou.bizdev.muse.adcoreweb.app.service.AdcoreWsService" factory-bean="adcoreWsFactory" factory-method="create" />  
  
    <bean id="adcoreWsFactory"  class="org.apache.cxf.jaxws.JaxWsProxyFactoryBean">  
        <property name="serviceClass" value="com.sogou.bizdev.muse.adcoreweb.app.service.AdcoreWsService" />  
        <property name="address" value="http://127.0.0.1:9990/webservice/adcore" />  
    </bean>
	
</beans>